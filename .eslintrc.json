{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console":0,
        "indent": 0,
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": 0,
        "semi": [
            "error",
            "always"
        ]
    }
}
